  z-index: 99;
}
.okBtn {
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  text-align: center;
  position: absolute;
  top: 440rpx;
  left: 0;
  color: #2796FF;
  letter-spacing: 3rpx;
  z-index: 99999;
}

.addressWindows { 
	width: 70%;
	background-color: transparent;
	position: fixed;
	top: 30%;
	left: 15%;
	padding: 15px;
	box-sizing: border-box;
	z-index: 999;
	border-radius: 5px;
	
}
.windows {
	width: 100%;
	height: 300px;
	background-color: #fff;
	position: fixed;
	bottom: 0;
	left: 0;
	padding: 15px;
	box-sizing: border-box;
	z-index: 999;
	border-radius: 5px;
	
}
.show {
	display: block;
}
.unshow {
	display: none;
}
.dark {
	height: 100%;
	width: 100%;
	z-index: 999;
	background-color: #000;
	opacity: 0.5;
	position: fixed;
	top: 0;
	left: 0;
}
