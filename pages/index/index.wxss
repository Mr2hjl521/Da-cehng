/**index.wxss**/
page {
  background-color: #fff;
}
.header {
  background-color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 40rpx;
  padding-bottom: 0;
}
.notice {
  width: 100%;
  height: 60rpx !important;
  line-height: 60rpx !important;
  padding: 0 20rpx;
  margin: 0 auto;
  box-sizing: border-box;
  background-color: #fff;
}
.noticeBar {
  border-radius: 10rpx;
  background-color: #FCE393 !important;
  height: 60rpx !important;
  line-height: 60rpx !important;
}
.noticeBar .van-notice-bar__wrap .van-notice-bar__content {
  height: 50rpx !important;
  line-height: 50rpx !important;
}
.hLeft {
  width: 500rpx;
}
.agentName {
  font-size: 36rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-bottom: 10rpx;
  display: inline-block;
  white-space: nowrap; 
  width: 100%; 
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.anLeft {
  width: 75%;
  background-color: #fff;
  overflow: hidden;
  text-overflow:ellipsis;
  letter-spacing: 2rpx;
  padding-left: 2rpx;
}
.anRight {
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
}
.agentAdress {
  font-size: 28rpx;
  letter-spacing: 1.5rpx;
  display: flex;
  flex-direction: row;
}
.adressImg {
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}
.adressImg image {
  width: 35rpx;
  height: 35rpx;
}
.right {
  width: 40rpx;
  height: 40rpx;
    margin-top: 10rpx;
}

.hRight {
  width: 110rpx;
  margin: auto 0;
}

.agentPic {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.swiper {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.swImg {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.categoryBox1 {
  padding: 20rpx;
  background-color: #fff;
}
.cBox {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}
.box {
