}

/* 分类栏2 */
.categoryBox2 {
  height: 120rpx;
  position: relative;
  margin-bottom: 30rpx;
}
.nav {
  height: 120rpx;
  line-height: 120rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: #EEEEEE;
  font-size: 16px;
  white-space: nowrap;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
  display: flex;
  flex-direction: row;
  justify-content: space-around;

}

.nav-item {
  padding: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx !important;
  display: inline-block;
  text-align: center;
  box-sizing: border-box;
}
.allbox {
  display: inline-block;
  height: 120rpx;
  line-height: 60rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
  text-align: center;
