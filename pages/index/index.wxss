} 
.view-icon{
  width:72%;
  height: 120rpx;
  background: #f2f2f2;
  /* background:red; */
  float: right;
  line-height: 120rpx;
  margin-top:25rpx;
}
.view-icon-text{
  float: left;
  width: 30%;
  height: 120rpx;
  background: yellow;
  font-size: 29rpx;
  line-height: 120rpx;
  text-align: center;
}










.nav-item.active{
  color: #FD599E;
}
