.box{
  width: 100%;
  height: 100%;
}
.box-view{
  width:100%;
  height:110rpx;
  background:rgba(255,58,52,1);
}
.van-cell{
  width: 88% !important;
  height: 80rpx;
  background: white;
  border-radius: 50rpx;
  margin-top: 13rpx;
  margin-left: 45rpx;
  float:left;
  border:none !important; 
}
.content{
  width: 100%;
  height: 300rpx;
  background: red;
}
.agentBox {
  border-bottom: 1rpx solid #E5E5E5;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}
.aLeft {
  width: 75%;
  display: flex;
  flex-direction: row;

}
.aRight {
  width: 22%;
}
.erweima {
  width: 80rpx;
  height: 80rpx;
  background-color: #ccc;
  margin: 10rpx auto;
}
  
.erweima image {
  width: 80rpx;
  height: 80rpx;
}
.smallFont2 {
  font-size: 21rpx;
  color: #686868;
  text-align: center;
}
.agentImg {
  width: 110rpx;
  height: 110rpx;
  margin-top: 40rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  /* border: 1rpx solid #B2B2B2 ; */
}
.agentImg image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}
.agentInfo {
  width: 400rpx;
  padding: 20rpx 0;
  font-size: 30rpx;
}
.name {
  font-size: 26rpx;
  margin-bottom: 5rpx;
}
.phone {
  color: #646464;
  font-size: 28rpx;
  margin-bottom: 5rpx;
}
.adress {
  font-size: 26rpx;
}
